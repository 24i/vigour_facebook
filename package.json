{
  "name": "vigour-facebook",
  "version": "1.1.2",
  "description": "facebook for vigorous apps!",
  "main": "lib/index.js",
  "scripts": {
    "test": "gaston test -r browser -s test/browser/index.js",
    "build": "cd example && ../node_modules/.bin/gaston build -s index.js -o ./ -i",
    "ios-test": "cd test && ../node_modules/.bin/gaston bundle -t -s manual/index.js -o . -i && wrapper build --native.selectedPlatforms ios",
    "ios": "npm run build && wrapper build --native.selectedPlatforms ios",
    "android-test": "cd test && ../node_modules/.bin/gaston bundle -t -s manual/index.js -o . -i && wrapper build --native.selectedPlatforms android",
    "android": "npm run build && wrapper build --native.selectedPlatforms android",
    "all": "npm run build && wrapper build",
    "tizentv": "npm run build && wrapper build --native.selectedPlatforms tizentv",
    "postinstall": "./makelinks.sh vigour-facebook"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vigour-io/facebook.git"
  },
  "keywords": [
    "vigour",
    "wrapper",
    "native",
    "plugin",
    "facebook"
  ],
  "author": "Vigour",
  "contributors": [
    "Shawn Inder <shawn@vigour.io>",
    "Marcus Besjes <marcus@vigour.io>",
    "Michiel Van Liempt <michiel@vigour.io>",
    "Alexander van de Werff <alex@vigour.io>"
  ],
  "bugs": {
    "url": "https://github.com/vigour-io/facebook/issues"
  },
  "homepage": "https://github.com/vigour-io/facebook",
  "engines": {
    "node": ">=4.1.2"
  },
  "dependencies": {
    "lodash": "3.10.x",
    "vigour-config": "~0.0.8",
    "vigour-env": "~1.0.19",
    "vigour-js": "~0.4.4",
    "vigour-ua": "~0.0.5",
    "vigour-wrapper": "~2.2.13"
  },
  "devDependencies": {
    "gaston": "^0.28.2",
    "npmlog": "^2.0.0",
    "vigour-element": "~0.2.4"
  },
  "license": "MIT",
  "vigour": {
    "native": {
      "platforms": {
        "tizentv": {
          "assets": {
            "example/build.html": true,
            "example/build.js": true,
            "example/build.css": true,
            "assets":true
          },
          "icons": {
            "ThumbIcon": "assets/img/appIcon(temporary).png",
            "BigThumbIcon": "assets/img/appIcon(temporary).png",
            "ListIcon": "assets/img/appIcon(temporary).png",
            "BigListIcon": "assets/img/appIcon(temporary).png"
          },
          "config": {
            "appName": "vigourexample",
            "iconPath": "icons/ThumbIcon.png",
            "yourDomain": "http://vigour.io/",
            "entryPoint": "example/build.html"
          }
        }
      }
    },
    "facebook": {
      "appId": "441539296031991",
      "scope": "public_profile,email",
      "web": {
        "version": "v2.5",
        "src": "https://connect.facebook.net/en_NL/sdk.js"
      }
    },
    "plugin": {
      "android": {
        "className": "io.vigour.plugin.facebook.FacebookPlugin",
        "instantiation": "new FacebookPlugin(this)",
        "metadata": {
          "android:name": "com.facebook.sdk.ApplicationId",
          "android:value": "@string/facebook_app_id"
        },
        "activity": {
          "android:name": "com.facebook.FacebookActivity",
          "android:configChanges": "keyboard|keyboardHidden|screenLayout|screenSize|orientation",
          "android:theme": "@android:style/Theme.Translucent.NoTitleBar",
          "android:label": "@string/app_name"
        },
        "strings": {
          "facebook_app_id": "441539296031991"
        }
      }
    }
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "expect",
      "before",
      "after",
      "beforeEach",
      "afterEach",
      "sinon",
      "FB"
    ]
  }
}
