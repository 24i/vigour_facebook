{
  "name": "vigour-facebook-example",
  "dependencies": {
    "vigour-facebook": "../"
  },
  "gaston": {
    "package": true
  },
  "vigour": {
    "facebook": {
      "appId": "1550949618559589",
      "scope": "public_profile,email",
      "web": {
        "version": "v2.5",
        "src": "https://connect.facebook.net/en_NL/sdk.js"
      }
    },
    "native": {
      "platforms": {
        "ios": {
          "productName": "Facebook Example",
          "organizationName": "Vigour",
          "organizationIdentifier": "io.vigour.facebook",
          "buildNumber": "1",
          "appIndexPath": "build.html",
          "NSAllowsArbitraryLoads": true,
          "assets": {
            "build.html": true,
            "build.js": true,
            "build.css": true
          }
        },
        "android": {
          "productName": "Facebook Example",
          "applicationId": "io.vigour.facebook",
          "appIndexPath": "build.html",
          "keystoreFile": "example.keystore",
          "keystorePassword": "shmexample",
          "keystoreAlias": "blabla",
          "assets": {
            "build.html": true,
            "build.js": true,
            "build.css": true
          }
        }
      }
    }
  }
}
